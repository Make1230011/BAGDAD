    jQuery(document).ready(function () {

        $.get(getLoginWndXMLUrl(), null, function (data) {
            $('div.extLogin:first').after(data);

            if ((window.location.href.split("#")[1]) == 'login') {
                Povar.actions.login();
            }

            jQuery('a.authorized').attr('onclick', '').each(function (index) {
                var $this = $(this);
                $this.attr('data-href', $this.attr('href'));
                $(this).attr('href', 'javascript:void(0);').unbind().bind('click', function () {
                    //jQuery('.login_close').addClass('login_header').html('ВХОД НА САЙТ');
                    var $this = $(this);
                    jQuery.cookie('deferredAfterLogin', $this.attr('data-href'));
                    Povar.actions.login();
                    var rel = $this.attr('rel');
                    if (rel && rel != 'nofollow') {
                        eval(rel);
                    } else {
                        jQuery('#loginTeaser').html('');
                    }
                })
            });//.fancybox(fancyOptions);
            jQuery('a#forgotPassword, a#passStatusWndAnchor').fancybox(fancyOptions);

            jQuery(".login_close").addClass('login_header').html('ВХОД НА САЙТ').bind("click", function () {
                jQuery("#login").fadeOut();
                closeFancyBox();
            });
            jQuery('#login_field').keypress(function (event) {
                if (event.keyCode == '13') {
                    event.preventDefault();
                    jQuery("#password_field").focus();

                }
            });
            jQuery('#password_field').keypress(function (event) {
                if (event.keyCode == '13') {
                    event.preventDefault();
                    login();
                }
            });
        });


    });

    function forgotPassword() {
        jQuery("#forgotPassForm").ajaxSubmit(function (data) {
            jQuery("#passStatusWnd div.wndContent:first").html(data);
            jQuery("a#passStatusWndAnchor").click();
        });
    }
    </script>

